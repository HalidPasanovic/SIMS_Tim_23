<Window x:Class="ZdravoCorp.View.ChangeRoom"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZdravoCorp.View"
        mc:Ignorable="d"
        Title="ChangeRoom" Height="450" Width="800">
    <Grid>
        <Label Content="Identifier" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Size" HorizontalAlignment="Left" Margin="10,51,0,0" VerticalAlignment="Top"/>
        <Label Content="RoomType" HorizontalAlignment="Left" Margin="10,97,0,0" VerticalAlignment="Top" Width="69"/>
        <TextBox x:Name="id" HorizontalAlignment="Left" Height="26" Margin="76,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="240"/>
        <TextBox x:Name="sz" HorizontalAlignment="Left" Height="26" Margin="76,51,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="240"/>
        <Button Content="Change" HorizontalAlignment="Left" Margin="20,128,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <TextBox x:Name="tp" HorizontalAlignment="Left" Height="26" Margin="79,97,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="240"/>

    </Grid>
</Window>
