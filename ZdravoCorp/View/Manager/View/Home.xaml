<UserControl x:Class="ZdravoCorp.View.Manager.View.Home"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ZdravoCorp.View.Manager.View"
             xmlns:p="clr-namespace:ZdravoCorp.View.Core"
             xmlns:svgc="http://sharpvectors.codeplex.com/svgc/"
             mc:Ignorable="d" 
             d:DesignHeight="667" d:DesignWidth="430">
    <Grid>
        <Button 
                    HorizontalAlignment="Left" 
                    Margin="30,81,0,0" 
                    VerticalAlignment="Top" 
                    Style="{DynamicResource WindowButton}" 
                    Height="130" 
                    Width="152"
                    Command="{Binding RoomsCommand}">
            <svgc:SvgViewbox Source="\Resources\Icons\rooms.svg" Width="107" Height="107"/>
        </Button>

        <Label Content="{p:Loc Rooms}" 
                   HorizontalAlignment="Left" 
                   Margin="71,203,0,0" 
                   VerticalAlignment="Top" 
                   HorizontalContentAlignment="Center"
                   FontSize="18"
                   Style="{DynamicResource ManagerLabels}" 
                   Height="35" Width="76"
                   Foreground="{DynamicResource ManagerLabelForeground}"/>

        <Button  
                    HorizontalAlignment="Left" 
                    Margin="254,81,0,0" 
                    VerticalAlignment="Top" 
                    Style="{DynamicResource WindowButton}" 
                    Height="130" 
                    Width="151" 
                    Command="{Binding EquipmentsCommand}">
            <svgc:SvgViewbox Source="\Resources\Icons\equipment.svg" Width="107" Height="107"/>
        </Button>

        <Label  Content="{p:Loc Equipment}" 
                    HorizontalAlignment="Left" 
                    Margin="279,205,0,0" 
                    VerticalAlignment="Top" 
                    HorizontalContentAlignment="Center" 
                    FontSize="18"
                    Style="{DynamicResource ManagerLabels}"
                Foreground="{DynamicResource ManagerLabelForeground}" Height="34" Width="105"/>

        <Button 
                    HorizontalAlignment="Left" 
                    Margin="30,244,0,0" 
                    VerticalAlignment="Top" 
                    Style="{DynamicResource WindowButton}" 
                    Height="130" 
                    Width="152"
                    Command="{Binding MedicationsCommand}">
            <svgc:SvgViewbox Source="\Resources\Icons\medicine.svg" Width="107" Height="107"/>
        </Button>

        <Label  Content="{p:Loc Medication}" 
                    HorizontalAlignment="Left" 
                    Margin="48,368,0,0" 
                    VerticalAlignment="Top" 
                    HorizontalContentAlignment="Center" 
                    FontSize="18"
                    Foreground="{DynamicResource ManagerLabelForeground}"
                Style="{DynamicResource ManagerLabels}" Height="34" Width="116"/>

        <Button 
                    HorizontalAlignment="Left" 
                    Margin="254,244,0,0" 
                    VerticalAlignment="Top" 
                    Style="{DynamicResource WindowButton}" 
                    Height="130" 
                    Width="151"
                    Command="{Binding SurveyCommand}">
            <svgc:SvgViewbox Source="\Resources\Icons\surveys.svg" Width="107" Height="107"/>
        </Button>

        <Label  Content="{p:Loc Surveys}" 
                    HorizontalAlignment="Left" 
                    Margin="285,368,0,0" 
                    VerticalAlignment="Top" 
                    HorizontalContentAlignment="Center" 
                    FontSize="18"
                Foreground="{DynamicResource ManagerLabelForeground}"
                    Style="{DynamicResource ManagerLabels}" Height="34" Width="89"/>

        <Button Content="Button" 
                    HorizontalAlignment="Left" 
                    Margin="30,400,0,0" 
                    VerticalAlignment="Top" 
                    Style="{DynamicResource WindowButton}" 
                    Height="130" 
                    Width="152"/>

        <Label  Content="{p:Loc Employess}" 
                    HorizontalAlignment="Left" 
                    Margin="51,522,0,0" 
                    VerticalAlignment="Top" 
                    HorizontalContentAlignment="Center" 
                    FontSize="18"
                Foreground="{DynamicResource ManagerLabelForeground}"
                    Style="{DynamicResource ManagerLabels}" Height="34" Width="110"/>

        <Button Content="Button" 
                    HorizontalAlignment="Left" 
                    Margin="254,400,0,0" 
                    VerticalAlignment="Top" 
                    Style="{DynamicResource WindowButton}" 
                    Height="130" 
                    Width="151"/>

        <Label  Content="{p:Loc Statistics}" 
                    HorizontalAlignment="Left" 
                    Margin="285,522,0,0" 
                    VerticalAlignment="Top" 
                    HorizontalContentAlignment="Center" 
                    FontSize="18"
                Foreground="{DynamicResource ManagerLabelForeground}"
                    Style="{DynamicResource ManagerLabels}" Height="34" Width="89"/>
    </Grid>
</UserControl>
