        <Label Content="Vrsta analize" Style="{DynamicResource PatientLabel}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="35,90,0,0"/>
        <Label Content="Datum" Style="{DynamicResource PatientLabel}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="35,120,0,0"/>
